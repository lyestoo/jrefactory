<?xml version="1.0"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.0//EN" "http://www.netbeans.org/dtds/filesystem-1_0.dtd">

<filesystem>
    <!-- Installing JRefactory menu actions: -->
    <folder name="Menu">
        <folder name="Tools">
            <folder name="JavaStyle">
               <file name="org-acm-seguin-ide-netbeans-PrettyPrinterAction.instance"/>
               <file name="org-acm-seguin-ide-netbeans-JRefactoryAction.instance"/>
               <folder name="Coding Standards">
                  <file name="org-acm-seguin-ide-netbeans-CSCheckBufferAction.instance"/>
                  <file name="org-acm-seguin-ide-netbeans-CSCheckAllBuffersAction.instance"/>
                  <file name="org-acm-seguin-ide-netbeans-CSCheckDirAction.instance"/>
                  <file name="org-acm-seguin-ide-netbeans-CSCheckDirRecursiveAction.instance"/>
               </folder>
               <folder name="Detect Duplicate Code">
                  <file name="org-acm-seguin-ide-netbeans-CPDCheckBufferAction.instance"/>
                  <file name="org-acm-seguin-ide-netbeans-CPDCheckAllBuffersAction.instance"/>
                  <file name="org-acm-seguin-ide-netbeans-CPDCheckDirAction.instance"/>
                  <file name="org-acm-seguin-ide-netbeans-CPDCheckDirRecursiveAction.instance"/>
               </folder>
               <folder name="Refactor">
                  <file name="org-acm-seguin-ide-netbeans-ExtractMethodAction.instance"/>
               </folder>
               <file name="org-acm-seguin-ide-netbeans-OptionsAction.instance"/>
            </folder>
        </folder>
    </folder>
</filesystem>
